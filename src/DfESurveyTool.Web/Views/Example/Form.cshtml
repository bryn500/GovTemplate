@model ExampleFormViewModel
<!-- Validation -->
<partial name="FormComponents/_ValidationSummary" model="ViewData.ModelState" />

<h1 class="govuk-heading-l ">
    Example form
</h1>

<form method="post" novalidate>    
    <partial name="FormComponents/_TextField" for="Text" />
    <partial name="FormComponents/_RadioButtonList" for="RadioQuestion" />
    <partial name="FormComponents/_RadioButtonList" for="RadioQuestion2" />
    <partial name="FormComponents/_CheckBoxList" for="CheckBoxQuestion" />
    <partial name="FormComponents/_CheckBoxList" for="CheckBoxQuestion2" />

    <!-- Submit -->
    <button class="button govuk-button" type="submit">Submit</button>
</form>
